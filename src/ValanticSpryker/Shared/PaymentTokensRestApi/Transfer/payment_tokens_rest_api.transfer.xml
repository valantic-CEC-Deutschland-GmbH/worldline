<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestPaymentTokensAttributes">
        <property name="tokens" type="RestPaymentTokenAttributes[]" singular="token"/>
    </transfer>

    <transfer name="RestPaymentTokenAttributes">
        <property name="token" type="string"/>
        <property name="expiryMonth" type="string"/>
        <property name="obfuscatedCardNumber" type="string"/>
        <property name="holderName" type="string"/>
        <property name="paymentMethod" type="string"/>
        <property name="expiredAt" type="string"/>
        <property name="deletedAt" type="string"/>
    </transfer>
</transfers>
